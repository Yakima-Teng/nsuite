{
  "source": {
    "include": ["./lib"],
    "includePattern": ".mjs$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-tsimport-plugin/index.js"
  ],
  "opts": {
    "destination": "./.docs",
    "recurse": true,
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "readme": "./README.md"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
